// Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id          Int        @id @default(autoincrement())
  title       String
  description String
  url         String
  price       Int
  created_at  DateTime?  @default(now()) @db.Timestamptz(6)
  orderItem   OrderItem[]
}
